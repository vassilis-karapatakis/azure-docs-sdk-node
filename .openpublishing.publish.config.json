{
  "docsets_to_publish": [
    {
      "docset_name": "node-api",
      "build_source_folder": ".",
      "build_output_subfolder": "node-api",
      "locale": "en-us",
      "moniker_ranges": [
        "azure-node-latest",
        "azure-node-preview",
        "azure-node-legacy"
      ],
      "open_to_public_contributors": true,
      "type_mapping": {
        "Conceptual": "Content",
        "ManagedReference": "Content",
        "RestApi": "Content",
        "UniversalReference": "Content",
        "TSType": "Content",
        "TSPackage": "Content",
        "TSEnum": "Content",
        "TSTypeAlias": "Content",
        "LandingData": "Content",
        "APIConnector": "Content",
        "ReferenceContainer": "Content"
      },
      "build_entry_point": "docs",
      "template_folder": "_themes",
      "version": 0,
      "customized_template_paths": [
        "_dependentPackages/uref.plugins/content"
      ],
      "DiffFolder": [
        "docs-ref-autogen/overview",
        "preview-packages/docs-ref-autogen/overview"
      ],
      "customized_tasks": {
        "docset_prebuild": [
          "_dependentPackages/CommonPlugins/tools/JoinTOC.ps1",
          "_dependentPackages/CommonPlugins/tools/DiffFolder.ps1"
        ]
      }
    }
  ],
  "notification_subscribers": [],
  "sync_notification_subscribers": [],
  "branches_to_filter": [],
  "git_repository_branch_open_to_public_contributors": "master",
  "skip_source_output_uploading": false,
  "need_preview_pull_request": true,
  "enable_incremental_build": false,
  "contribution_branch_mappings": {},
  "dependent_repositories": [
    {
      "path_to_root": "_themes",
      "url": "https://github.com/Microsoft/templates.docs.msft",
      "branch": "master",
      "branch_mapping": {}
    },
    {
      "path_to_root": "azure-sdk-for-js",
      "url": "https://github.com/Azure/azure-sdk-for-js",
      "branch": "master",
      "branch_mapping": {}
    }
  ],
  "branch_target_mapping": {},
  "need_generate_pdf_url_template": false,
  "targets": {},
  "need_generate_pdf": false,
  "need_generate_intellisense": false,
  "dependent_packages": [
    {
      "id": "opbuild.templates.uref",
      "nuget_feed": "https://www.myget.org/F/op/api/v2",
      "path_to_root": "_dependentPackages/uref.plugins",
      "target_framework": "net45",
      "version": "latest"
    },
    {
      "id": "Microsoft.OpenPublishing.CommonPlugins",
      "nuget_feed": "https://www.myget.org/F/op/api/v2",
      "path_to_root": "_dependentPackages/CommonPlugins",
      "target_framework": "net45",
      "version": "latest"
    }
  ],
  "JoinTOCPlugin": [
    {
      "TopLevelTOC": "docs-ref-mapping/reference.yml",
      "ReferenceTOC": "docs-ref-autogen/toc.yml",
      "ReferenceTOCUrl": "/javascript/api/azure_node_ref_toc/toc.json",
      "ConceptualTOCUrl": "/azure/javascript/toc.json",
      "OutputFolder": "docs-ref-autogen/overview",
      "HideEmptyNode": false,
      "ContainerPageMetadata": {
        "langs": [
          "js"
        ]
      }
    },
    {
      "TopLevelTOC": "preview-packages/docs-ref-mapping/reference.yml",
      "ReferenceTOC": "preview-packages/docs-ref-autogen/toc.yml",
      "ConceptualTOCUrl": "/azure/javascript/toc.json",
      "ReferenceTOCUrl": "/javascript/api/azure_node_ref_toc_preview/toc.json?view=azure-node-preview",
      "OutputFolder": "preview-packages/docs-ref-autogen/overview",
      "HideEmptyNode": false,
      "ContainerPageMetadata": {
        "langs": [
          "js"
        ]
      }
    },
    {
      "TopLevelTOC": "previous-packages/docs-ref-mapping/reference.yml",
      "ReferenceTOC": "previous-packages/docs-ref-autogen/toc.yml",
      "ConceptualTOCUrl": "/azure/javascript/toc.json",
      "OutputFolder": "previous-packages/docs-ref-autogen/overview",
      "HideEmptyNode": false,
      "ContainerPageMetadata": {
        "langs": [
          "js"
        ]
      }
    }
  ],
  "docs_build_engine": {
    "name": "docfx_v3"
  }
}
